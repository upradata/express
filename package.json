{
    "name": "@upradata/express",
    "author": {
        "name": "Thomas Milotti",
        "email": "thomas.milotti@gmail.com",
        "url": "https://github.com/milottit"
    },
    "repository": {
        "url": "milottit@vs-ssh.visualstudio.com:v3/milottit/Util%20Typescript/Util%20Typescript"
    },
    "license": "MIT",
    "version": "1.0.24",
    "description": "Set of typescript util tools",
    "main": "lib",
    "types": "lib-esm",
    "module": "lib-esm",
    "files": [
        "lib",
        "lib-esm"
    ],
    "scripts": {
        "test": "npx jest --verbose false --config jest-ut.config.js",
        "e2e": "npx jest --passWithNoTests --verbose false --config jest-e2e.config.js",
        "clean": "shx rm -rf lib lib-esm",
        "prebuild": "npm run clean",
        "build": "tsc && tsc -m es6 --outDir lib-esm",
        "watch": "concurrently 'tsc -w' 'tsc -w -m es6 --outDir lib-esm'",
        "postbuild": "npm run test && npm run e2e",
        "build:only": "npm run prebuild && tsc && tsc -m es6 --outDir lib-esm",
        "build:watch": "npm run tsc -w",
        "github-push": "npm run build && npm version patch && git push && git push --tags",
        "npm-publish": "npm run build && npm publish --access public"
    },
    "dependencies": {
        "@google-cloud/debug-agent": "^4.2.2",
        "@sendgrid/client": "^7.1.1",
        "body-parser": "^1.19.0",
        "cors": "^2.8.5",
        "express": "^4.17.1",
        "serve-static": "^1.14.1"
    },
    "devDependencies": {
        "@types/body-parser": "^1.19.0",
        "@types/cors": "^2.8.6",
        "@types/express": "^4.17.6",
        "@types/express-serve-static-core": "^4.17.7",
        "@types/mailgun-js": "^0.22.7",
        "@types/node": "^14.0.5",
        "@types/qs": "^6.9.3",
        "@types/serve-static": "^1.13.4",
        "concurrently": "^5.2.0",
        "mailgun-js": "^0.22.0",
        "tsconfig-paths": "^3.9.0"
    },
    "local": {
        "dependencies": {
            "@upradata/util": "link:/home/milottit/Libraries/Util/util@1.0.24",
            "@upradata/node-util": "link:/home/milottit/Libraries/Util/node-util@2.0.3"
        }
    }
}
